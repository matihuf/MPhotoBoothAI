<UserControl xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:dvm="using:MPhotoBoothAI.Avalonia.Design.ViewModels"
        xmlns:vm="using:MPhotoBoothAI.Application.ViewModels"
        xmlns:c="using:MPhotoBoothAI.Avalonia.Controls"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="MPhotoBoothAI.Avalonia.Views.HomeView"
        x:DataType="vm:HomeViewModel">

  <Design.DataContext>
    <dvm:DesignHomeViewModel/>
  </Design.DataContext>
    
  <c:LanguageControl DataContext="{Binding Language}"/>
</UserControl>
